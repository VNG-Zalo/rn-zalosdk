{
    "name": "RNZaloSDKExample",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "npx react-native run-android",
        "ios": "npx react-native run-ios",
        "start": "npx react-native start",
        "reset": "npx react-native start --reset-cache",
        "test": "npx jest",
        "lint": "npx eslint .",
        "clean": "watchman watch-del-all && rm -rf node_modules && npx yarn cache clean && npx yarn install",
        "clean_ios": "cd ios && rm -rf Pods && rm -f Podfile.lock && rm -rf RNZaloAuth.xcworkspace && npx pod install && open RNZaloAuth.xcworkspace",
        "pretty": "prettier --write \"src/**/*.js\"",
        "build:ios": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
        "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle"
    },
    "dependencies": {
        "@babel/runtime": "^7.8.4",
        "@react-native-community/masked-view": "^0.1.7",
        "@react-navigation/native": "^5.0.9",
        "react": "16.9.0",
        "react-native": "0.61.0",
        "react-native-button": "^2.4.0",
        "react-native-gesture-handler": "^1.6.0",
        "react-native-message-bar": "^2.0.10",
        "react-native-reanimated": "^1.7.0",
        "react-native-router-flux": "^4.2.0",
        "react-native-safe-area-context": "^0.7.3",
        "react-native-screens": "^2.3.0",
        "rn-zalo": "../.."
    },
    "devDependencies": {
        "@babel/core": "^7.8.4",
        "@babel/preset-env": "^7.8.4",
        "@react-native-community/eslint-config": "^0.0.5",
        "appium-doctor": "^1.13.1",
        "babel-jest": "^25.1.0",
        "babel-preset-react-native": "^4.0.1",
        "eslint": "^6.5.1",
        "jest": "^25.1.0",
        "metro-react-native-babel-preset": "^0.56.0",
        "react-dom": "^16.12.0"
    }
}
